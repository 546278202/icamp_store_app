<view class="container">
    <view style="color:#303030;font-size: 14px;overflow: hidden;">
        <navigator hover-class="none" url="../campPhoto/index?projectId={{item.id}}&camperId={{item.camperId}}" wx:for="{{dataList}}" wx:for-item="item" wx:for-index='index' style="padding:15px;margin-bottom: 10px;background:rgba(255,255,255,1);">
            <view style="display: flex; align-items: center;justify-content: space-between;margin-bottom: 10px;">
                <text style="color:#303030;font-size: 14px;">{{item.attributeValueName}}</text>
                <van-icon name="arrow" color="#BBC0CA" />
            </view>
            <view style="display:flex;align-items: center;justify-content: space-between;">
                <image src="../../image/tab4/4.png" style="height:50px;width:50px;" mode='widthFix' />
                <image src="../../image/tab4/4.png" style="height:50px;width:50px;" mode='widthFix' />
                <image src="../../image/tab4/4.png" style="height:50px;width:50px;" mode='widthFix' />
                <image src="../../image/tab4/4.png" style="height:50px;width:50px;" mode='widthFix' />
                <image src="../../image/tab4/4.png" style="height:50px;width:50px;" mode='widthFix' />
            </view>
        </navigator>
    </view>
    <view wx:if="{{dataList.length==0}}">
        <image src="https://tstore.i-camp.com.cn/images/201911131617582718155.png" style="height:499px;" mode='aspectFill' binderror="avatarError" />
        <view style="color:#302C48;font-size: 20px;text-align: center;margin-bottom:10px;">Oh oh!</view>
        <view style="color:#302C48;font-size: 15px;font-weight:400;text-align: center;font-family:PingFang-SC-Regular,PingFang-SC;">还没有营会</view>
    </view>
</view>